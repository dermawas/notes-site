<svg xmlns="http://www.w3.org/2000/svg" width="64" height="64" viewBox="0 0 64 64">
  <!-- background -->
  <rect width="64" height="64" rx="12" fill="#f3f4f6"></rect>

  <!-- radiant spark (12 rays for balance) -->
  <g transform="translate(32,28)" opacity="0.25" stroke="#3b82f6" stroke-width="2" stroke-linecap="round" fill="none">
    <circle r="5" fill="#3b82f6" stroke="none"></circle>
    <g>
      <!-- radial rays -->
      <line x1="0" y1="-12" x2="0" y2="-20"></line>
      <line x1="0" y1="12" x2="0" y2="20"></line>
      <line x1="12" y1="0" x2="20" y2="0"></line>
      <line x1="-12" y1="0" x2="-20" y2="0"></line>
      <line x1="8" y1="-8" x2="14" y2="-14"></line>
      <line x1="-8" y1="8" x2="-14" y2="14"></line>
      <line x1="-8" y1="-8" x2="-14" y2="-14"></line>
      <line x1="8" y1="8" x2="14" y2="14"></line>
      <line x1="4" y1="-12" x2="7" y2="-20"></line>
      <line x1="-4" y1="12" x2="-7" y2="20"></line>
      <line x1="-12" y1="4" x2="-20" y2="7"></line>
      <line x1="12" y1="-4" x2="20" y2="-7"></line>
    </g>
  </g>

  <!-- FFL letters -->
  <text x="32" y="46" font-family="Inter, system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif" font-size="20" font-weight="700" text-anchor="middle" letter-spacing="1" fill="#374151">
    FFL
  </text>
</svg>
